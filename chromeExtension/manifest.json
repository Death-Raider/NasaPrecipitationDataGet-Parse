{
  "name": "darshTest",
  "version":"1",
  "manifest_version": 2,
  "description": "Extension Version 4.0",
  "permissions": [
    "unlimitedStorage",
    "tabs",
    "activeTab",
    "storage",
    "downloads",
    "downloads.open",
    "cookies",
    "alarms",
    "https://search.earthdata.nasa.gov/*",
    "http://search.earthdata.nasa.gov/*",
    "https://urs.earthdata.nasa.gov/*",
    "https://cmr.earthdata.nasa.gov/search/*"
  ],
  "web_accessible_resources": ["/images/*", "js/LZStringWorker.js"],
  "background": {
    "scripts": [
      "JS/test.js"
    ]
  },
  "content_security_policy": "script-src 'self'  ; object-src 'self'"
}
